{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [ "plugin:cypress/recommended",  "airbnb-base" ],

    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["import"],
    "rules": {
      "import/extensions": "off",
      "indent": ["error", 4],
      //"no-return-assign": ["except-parens", 2],
        // "no-param-reassign": ["error", { "props": true, "ignorePropertyModificationsFor": ["el.textContent"] }]
        "no-param-reassign": "off"
    }
}
