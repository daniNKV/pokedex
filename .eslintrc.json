{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", "tab"],
        //"no-return-assign": ["except-parens", 2],
        // "no-param-reassign": ["error", { "props": true, "ignorePropertyModificationsFor": ["el.textContent"] }]
        "no-param-reassign": "off",
        "no-tabs": ["error", { "allowIndentationTabs": true }]


    }
}
